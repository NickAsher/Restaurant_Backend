{{#> template_base}}
    <!--Page Title-->
    <div class="app-page-title">
        <div class="page-title-wrapper">
            <div class="page-title-heading">
                <div class="page-title-icon">
                    <i class="fa fa-phone icon-gradient bg-amy-crisp"></i>
                </div>
                <div>Contact Info
                    <div class="page-title-subheading">View and Edit the Restaurant Contact Info here
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--Page Title /End-->





    <div class="card">
        <div class="card-header">
            <h3 class="ytext-heading">Address</h3>
        </div>
        <div class="card-body">
            <div class="form-group row">
                <label for="input-rest-name" class="col-3 col-form-label">Restaurant Name</label>
                <div class="col-md-9">
                    <input class="form-control" type="text" placeholder="ex. My Restaurant" id="input-rest-name" value="{{contactData.restaurant_name}}" disabled>
                </div>
            </div>

            <div class="form-group row">
                <label for="input-rest-addr1" class="col-3 col-form-label">Address Line 1 </label>
                <div class="col-md-9">
                    <input class="form-control" type="text" placeholder="" id="input-rest-addr1" value="{{contactData.restaurant_addr_1}}" disabled>
                </div>
            </div>

            <div class="form-group row">
                <label for="input-rest-addr2" class="col-3 col-form-label">Address Line 2</label>
                <div class="col-md-9">
                    <input class="form-control" type="text" placeholder="" id="input-rest-addr2" value="{{contactData.restaurant_addr_2}}" disabled>
                </div>
            </div>

            <div class="form-group row">
                <label for="input-rest-addr3" class="col-3 col-form-label">Address Line 3</label>
                <div class="col-md-9">
                    <input class="form-control" type="text" placeholder="" id="input-rest-addr3" value="{{contactData.restaurant_addr_3}}" disabled>
                </div>
            </div>


        </div>
    </div>
    <br><br>


    <div class="row">
        <div class="col-6">
            <div class="card">

                <div class="card-header">
                    <h3 class="ytext-heading">Contact</h3>
                </div>

                <div class="card-body">
                    <div class="form-group row">
                        <label for="input-rest-phone" class="col-3 col-form-label">Phone no</label>
                        <div class="col-md-9">
                            <input class="form-control" type="text" placeholder="ex. 9780045712" id="input-rest-phone" value="{{contactData.restaurant_phone}}" disabled>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="input-rest-email" class="col-3 col-form-label">Email Id</label>
                        <div class="col-md-9">
                            <input class="form-control" type="text" placeholder="ex. example@gmail.com" id="input-rest-email" value="{{contactData.restaurant_email}}" disabled>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div class="col-6">
            <div class="card">
                <div class="card-header">
                    <h3 class="ytext-heading">Google Maps</h3>
                </div>

                <div class="card-body">
                    <div class="form-group row">
                        <label for="input-rest-latitude" class="col-3 col-form-label">Latitude</label>
                        <div class="col-md-9">
                            <input class="form-control" type="text" placeholder="ex. 94.147" id="input-rest-latitude" value="{{contactData.latitude}}" disabled>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="input-rest-longitude" class="col-3 col-form-label">Longitude</label>
                        <div class="col-md-9">
                            <input class="form-control" type="text" placeholder="ex. 120.745" id="input-rest-longitude" value="{{contactData.longitude}}" disabled>
                        </div>
                    </div>

                </div>
            </div>
        </div>

    </div>
    <br><br>


    <div class="card">
        <div class="card-header">
            <h3 class="ytext-heading">Social Media Links</h3>
        </div>
        <div class="card-body">


            <div class="form-group row">
                <label for="input-socialMedia-facebook" class="col-3 col-form-label">Facebook</label>
                <div class="col-md-9">
                    <input id="input-socialMedia-facebook" class="form-control" type="text" value="{{contactData.social_info.facebook}}" disabled>
                </div>
            </div>

            <div class="form-group row">
                <label for="input-socialMedia-instagram" class="col-3 col-form-label">Instagram</label>
                <div class="col-md-9">
                    <input id="input-socialMedia-instagram" class="form-control" type="text" value="{{contactData.social_info.instagram}}" disabled>
                </div>
            </div>

            <div class="form-group row">
                <label for="input-socialMedia-twitter" class="col-3 col-form-label">Twitter</label>
                <div class="col-md-9">
                    <input id="input-socialMedia-twitter" class="form-control" type="text" value="{{contactData.social_info.twitter}}" disabled>
                </div>
            </div>

            <div class="form-group row">
                <label for="input-socialMedia-youtube" class="col-3 col-form-label">Youtube </label>
                <div class="col-md-9">
                    <input id="input-socialMedia-youtube" class="form-control" type="text" value="{{contactData.social_info.youtube}}" disabled>
                </div>
            </div>


        </div>
    </div>
    <br><br>

    <div class="form-group row">
        <div class="col-4" ></div>
        <a class="col-4 btn btn-primary" href="/contact/edit" > Edit</a>
        <div class="col-4" ></div>
    </div>
    <br><br>

{{/template_base}}